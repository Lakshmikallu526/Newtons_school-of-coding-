f=0
l=[]
t=int(input())
for i in range(0,t):
 n=int(input())
 for i in range(1,n):
  for j in range(1,n):
   for k in range(1,n):
    if (i+j+k==n) and ((i^j^k)==0):
     f=1
     break
   break
  break
 
   
 if f==0:
  print(-1)
 else:
  print(i,end=' ')
  print(j,end=' ')
  print(k,end=' ')
  print()
